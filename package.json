{
  "name": "typia",
  "version": "6.0.5",
  "description": "Superfast runtime validators with only one line",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.mts",
  "bin": {
    "typia": "./lib/executable/typia.js"
  },
  "scripts": {
    "test": "npm run package:tgz",
    "test:template": "npm run test template",
    "-------------------------------------------------": "",
    "build": "tsup",
    "dev": "tsup --watch",
    "eslint": "eslint ./**/*.ts",
    "eslint:fix": "eslint ./**/*.ts --fix",
    "prettier": "prettier src --write",
    "------------------------------------------------": "",
    "package": "tsx deploy/index.ts",
    "package:latest": "npm run package latest",
    "package:next": "npm run package next",
    "package:patch": "npm run package patch",
    "package:tgz": "npm run package tgz",
    "package:deprecate": "npm deprecate typescript-json \"Renamed to typia\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/typia"
  },
  "keywords": [
    "fast",
    "json",
    "stringify",
    "typescript",
    "transform",
    "ajv",
    "io-ts",
    "schema",
    "jsonschema",
    "generator",
    "assert",
    "clone",
    "is",
    "validate",
    "equal",
    "runtime",
    "type",
    "typebox",
    "checker",
    "validator",
    "safe",
    "parse",
    "prune",
    "random"
  ],
  "author": "Jeongho Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/typia/issues"
  },
  "homepage": "https://typia.io",
  "dependencies": {
    "@samchon/openapi": "0.1.22",
    "commander": "^10.0.0",
    "comment-json": "^4.2.3",
    "inquirer": "^8.2.5",
    "randexp": "^0.5.3",
    "tsx": "^4.11.2"
  },
  "peerDependencies": {
    "typescript": ">=4.8.0 <5.5.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.46.2",
    "@swc/core": "^1.5.24",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/inquirer": "^8.2.5",
    "@types/node": "^18.15.12",
    "@types/ts-expose-internals": "npm:ts-expose-internals@5.4.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "chalk": "^4.0.0",
    "fast-glob": "^3.3.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "suppress-warnings": "^1.0.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "stackblitzs": {
    "startCommand": "npm install && npm run test"
  },
  "files": [
    "LICENSE",
    "README.md",
    "package.json",
    "lib",
    "src"
  ],
  "private": true
}
